                ""
              ]
            },
            "title": "Zeitreihe"
          },
          {
            "name": "sex",
            "type": "string",
            "constraints": {
              "enum": [
                "Weiblich",
                "Männlich",
                ""
              ]
            },
            "title": "Geschlecht"
          },
          {
            "name": "Value",
            "type": "number",
            "title": "Value"
          }
        ]
      }
    }
  ],
  "profile": "data-package",
  "name": "3-a-1",
